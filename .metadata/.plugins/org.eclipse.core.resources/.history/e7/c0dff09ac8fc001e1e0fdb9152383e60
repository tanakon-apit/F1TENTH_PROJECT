#include "RC_Signal.h"

uint8_t RC_Signal_Init(
		RC_Signal_Structure *RCx,
		float cpu_freq,
		float pwm_freq,
		bool isCHN
)
{
	HAL_TIM_Base_Start(RCx->htimx);
	if (isCHN) HAL_TIMEx_PWMN_Start(RCx->htimx, RCx->channelx);
	else HAL_TIM_PWM_Start(RCx->htimx, RCx->channelx);


}
